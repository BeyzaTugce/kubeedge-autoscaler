[Unit]
Description=Proxy service for request forwarding
After=network.target

[Service]
User=ubuntu
WorkingDirectory=/home/ubuntu/modules/
ExecStart=/home/ubuntu/.local/bin/gunicorn -b 0.0.0.0:8280 -w 1 proxy:app
Restart=always
StartLimitInterval=0
RestartSec=10

[Install]
WantedBy=multi-user.target